{
  "name": "qcs",
  "version": "0.2.0-0",
  "description": "Queue for answering questions",
  "repository": {
    "type": "git",
    "url": "https://github.com/florayann/qcs"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "dev": "webpack --config webpack-dev.config.js --progress --colors",
    "build": "webpack --config webpack-production.config.js --progress --colors",
    "test": "jest"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.16.0",
    "enzyme": "^2.5.1",
    "html-webpack-plugin": "^2.7.2",
    "jest": "^16.0.2",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.6",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "json-loader": "^0.5.4",
    "material-ui": "^0.15.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-tap-event-plugin": "^1.0.0",
    "seedrandom": "^2.4.2",
    "react-flip-move": "^2.4.2",
    "underscore": "1.8.3",
    "tinycolor2": "1.4.1",
    "visibilityjs": "1.2.4",
    "moment": "^2.15.1"
  },
  "jest": {
    "verbose": true,
    "automock": true,
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "react",
      "node_modules/enzyme/",
      "node_modules/material-ui/",
      "node_modules/sinon/",
      "node_modules/underscore"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "rootDir": "frontend",
    "testPathIgnorePatterns": [".*TestData.js",
                               "#.*"
                              ]
  }
}
